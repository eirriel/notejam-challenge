[
  {
    "name": "<APP_NAME>",
    "image": "<IMAGE_NAME>",
    "cpu": 1,
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": <SERVICE_PORT>
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "log-opts": {
        "awslogs_region": "ap-southeast-2",
        "awslogs-group": "notejam-logs",
        "awslogs-create-group": true
      }
    }
  }
]
