[
  {
    "name": "<APP_NAME>",
    "image": "<IMAGE_NAME>",
    "cpu": 1,
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": <SERVICE_PORT>
      }
    ],
    "log-driver": "awslogs",
    "log-opts": {
      "awslogs_region": "${AWS_DEFAULT_REGION}",
      "awslogs-group": "notejam-logs",
      "awslogs-create-group": true
    }
  }
]
