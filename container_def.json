[
  {
    "name": "notejam",
    "image": "${REPOSITORY_URI}:${TAG}",
    "cpu": 1,
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 5000
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "log-opts": {
        "awslogs_region": "${AWS_DEFAULT_REGION}",
        "awslogs-group": "notejam-logs",
        "awslogs-create-group": true
      }
    }
  }
]
